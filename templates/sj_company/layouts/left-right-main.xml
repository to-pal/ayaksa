<?xml version="1.0" encoding="utf-8"?>
<layout type="lrm">	
	<head>
	</head>
	<groups>
		<group class="span4" data-normal="" data-tablet="" data-stablet="">left</group>
		<group class="span3" data-normal="" data-tablet="" data-stablet="" noleft-class="first-child">right</group>
		<group 
			   class="span5 " 
			   data-normal="span5" 
			   data-stablet="span5"
			   
			   noright-class="span8"
			   noright-data-normal="" 
			   noright-data-stablet="span8"
			   
			   noleft-class="span9"
			   noleft-data-normal="" 
			   noleft-data-stablet=""
			   
			   noleftright-class="span12"
			   noleftright-data-normal="" 
			   noleftright-data-tablet="" 
			   noleftright-data-stablet="">main</group>
	</groups>
	<blocks>			
		<header html5tag="header" order="1" id="yt_header" autosize="0">
			<positions>
				<position class="logo" type="feature">@logo</position>
				<position class="span4 offset4" data-wide="span4 offset4" data-mobile="span3 offset3" style="none" type="modules">position-0</position>				
			</positions>
		</header>
		
		<menu html5tag="section" order="2" id="yt_menuwrap">
			<positions>
				<position class="span9"  type="feature">@menu</position>
				<position class="span3"  style="none" type="modules">position-1</position>
			</positions>
		</menu>
		
		<slideshow html5tag="section" order="3" id="yt_slideshow">
			<positions>
				<position class="span12" style="ytmod" type="modules">slide_show</position>
			</positions>
		</slideshow>
		
		<breadcrumb html5tag="section" order="4" id="yt_breadcrumb">
			<positions>
				<position class="span12" style="ytmod" type="modules">breadcrumb</position>
			</positions>
		</breadcrumb>
		
		<spotlight html5tag="section" order="5" id="yt_spotlight">
			<positions>
				<position class="span4" data-normal="" data-stablet="" data-mobile="" style="ytmod" type="modules">position-a</position>
				<position class="span4" data-normal="" data-stablet="" data-mobile="" style="ytmod" type="modules">position-b</position>
				<position class="span4" data-normal="" data-stablet="" data-mobile="" style="ytmod" type="modules">position-c</position>
				
			</positions>
		</spotlight>
		
		<content html5tag="section" order="6" id="content" showDivTop="0" showDivBottom="0">
			<positions>
				<position class="span12" data-normal="" data-tablet="" data-stablet="" group="left" style="ytmod" type="modules">position-5</position>
				<position class="span12" data-normal="" data-tablet="" data-stablet="" group="right" style="ytmod" type="modules">position-6</position>
				<position 
					class="span12" 
					data-normal="" 
					data-stablet=""
					
					noleft-class="span12" 
					noleft-data-normal="" 
					noleft-data-tablet="" 
					noleft-data-stablet="" 
					
					noright-class="span12" 
					noright-data-normal="" 
					noright-data-tablet="" 
					noright-data-stablet="" 
					
					noleftright-class="span12" 
					noleftright-data-normal="" 
					noleftright-data-tablet="" 
					noleftright-data-stablet="" 
					
					group="main" type="message"></position>
				<position 
					class="span12" 
					data-normal="" 
					data-stablet=""
					
					noleft-class="span12" 
					noleft-data-normal="" 
					noleft-data-tablet="" 
					noleft-data-stablet="" 
					
					noright-class="span12" 
					noright-data-normal="" 
					noright-data-tablet="" 
					noright-data-stablet="" 
					
					noleftright-class="span12" 
					noleftright-data-normal="" 
					noleftright-data-tablet="" 
					noleftright-data-stablet="" 
					 
					group="main" type="component">component</position>
				
				<position
					class="span12"
					data-normal="span12"
					data-stablet="span12" 
					  
					noright-class="span12" 
					noright-data-normal="" 
					noright-data-stablet=""
					  
					group="main" style="ytmod" type="modules">position-e</position>
				
			</positions>
		</content>
		
		<spotlight2 html5tag="section" order="7" id="yt_spotlight2">
			<positions>
				<position class="span12" data-normal="" data-stablet="" style="ytmod" type="modules">position-7</position>
			</positions>
		</spotlight2>
		
		<spotlight3 html5tag="section" order="8" id="yt_spotlight3">
			<positions>
				<position class="span12" data-normal="" data-stablet="" style="ytmod" type="modules">position-8</position>
			</positions>
		</spotlight3>
		
		<spotlight4 html5tag="section" order="9" id="yt_spotlight4">
			<positions>
				<position class="span12" data-normal="" data-stablet="" style="ytmod" type="modules">position-9</position>
			</positions>
		</spotlight4>
		
		<spotlight5 html5tag="section" order="10" id="yt_spotlight5">
			<positions>
				<position class="span3" data-normal="" data-stablet="span4" style="ytmod" type="modules">position-10</position>
				<position class="span5" data-normal="" data-stablet="span4" style="ytmod" type="modules">position-11</position>
				<position class="span4" data-normal="" data-stablet="span4" style="ytmod" type="modules">position-12</position>
			</positions>
		</spotlight5>
		
		<footer html5tag="footer" order="11" id="yt_footer">
			<positions>
				<position class="span12" type="feature">@copyright</position>				
			</positions>
		</footer>										
	</blocks>
</layout>		